        android:layout_height="wrap_content"
        android:text="青春不散场" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="富婆。。。。。" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="不想努力了~~" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="架构师" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="毕业季"
        android:textSize="29dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="青春不散场" />

